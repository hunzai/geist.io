                            },
                            {
                                "name": "from",
                                "selector": "td:nth-child(8) > div > div > a",
                                "type": "text"
                            },
                            {
                                "name": "to",
                                "selector": "td:nth-child(10) > div > div > a",
                                "type": "text"
                            },
                            {
                                "name": "to_link",
                                "selector": "td:nth-child(10) > div > div > a",
                                "type": "attribute",
